    "dekeywordify": "^0.2.1",
    "eslint": "^0.21.0",
    "expect.js": "^0.3.1",
    "istanbul": "^0.3.14",
    "mocha": "^2.2.4",
    "uglify-js": "^2.4.21",
    "watch": "^0.16.0"
  },
  "dependencies": {
    "extend": "^2.0.1",
    "linkedlist": "^1.0.1"
  },
